<?xml version="1.0" encoding="UTF-8"?>

<csa:uamapping xmlns:csa="https://github.com/ChimeraTK/ControlSystemAdapter-OPC-UA-Adapter">
  <config>
    <server applicationName="ChimeraTK-GenericServer" port="4840"/>
    <security unsecure="false" certificate="opcua_certs/server_cert.der" privatekey="opcua_certs/server_key.der" trustlist="opcua_certs/trust" blocklist="opcua_certs/blocklists" issuerlist="opcua_certs/issuer" />
    <login username="user" password="test"/>
  </config>
  <process_variable sourceName="msTimer/tick" copy="true">
      <name>triggerCounter</name>
      <destination>NewFolder</destination>
  </process_variable>
</csa:uamapping>
